{
  "name": "wee-url",
  "version": "0.0.1",
  "description": "Url shortener API",
  "main": "dist/lambda.js",
  "type": "module",
  "scripts": {
    "start": "docker compose up",
    "start:api": "ts-node-dev --respawn src/app.local.ts",
    "init-db": "node scripts/initialise-db.js",
    "build": "tsc",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "check-types": "tsc --noEmit",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AllyMurray/wee-url.git"
  },
  "author": "Ally Murray",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AllyMurray/wee-url/issues"
  },
  "homepage": "https://github.com/AllyMurray/wee-url#readme",
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
    "@vendia/serverless-express": "^4.1.3",
    "aws-sdk": "^2.839.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "http-status-codes": "^2.1.4",
    "inversify": "^5.0.5",
    "inversify-express-utils": "^6.3.2",
    "luxon": "^1.25.0",
    "pino": "^6.11.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.20",
    "@types/luxon": "^1.25.1",
    "@types/node": "^14.14.22",
    "@types/pino": "^6.3.5",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
